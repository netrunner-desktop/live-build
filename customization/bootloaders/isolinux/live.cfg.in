label live-@FLAVOUR@
	menu label ^Start Netrunner Core Live (@FLAVOUR@)
	menu default
        linux @LINUX@
        initrd @INITRD@
        append @APPEND_LIVE@

label live-@FLAVOUR@-failsafe
	menu label ^Start Netrunner Core Live (@FLAVOUR@ failsafe)
        linux @LINUX@
        initrd @INITRD@
        append @APPEND_LIVE_FAILSAFE@

